#/usr/bin/python
import sys, re, urllib2

url=urllib2.urlopen("http://localhost:8080/lines")
data=url.read()
url.close()

config=False
if len(sys.argv) >= 2:
    config=True
for line in data.split("\n"):
    line2=re.sub("[\#\:-]", "", line)
    if not line2:
        continue
    if not config:
        print line2
    else:
        print re.sub("\.", "", line2.split(" ")[0])+".label", line2.split(" ")[0]
        print re.sub("\.", "", line2.split(" ")[0])+".min", 0
